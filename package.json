{
  "name": "simple_crud_api",
  "version": "1.0.0",
  "description": "Simple HTTP crud API",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && webpack --config ./webpack.config.js",
    "test": "jest --testPathPattern=__tests__",
    "start:dev": "NODE_ENV=development nodemon --exec ts-node index.ts",
    "start:prod": "NODE_ENV=production node ./bundle/index.js",
    "start:multi": "NODE_ENV=development nodemon --exec ts-node multi.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyonic/simple_crud_api.git"
  },
  "keywords": [
    "http",
    "load",
    "balancer"
  ],
  "author": "@pyonic",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pyonic/simple_crud_api/issues"
  },
  "dependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "homepage": "https://github.com/pyonic/simple_crud_api#readme"
}
